{
  "name": "agin-requests",
  "displayName": "Agin Requests",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "agin-requests.new",
        "title": "Agin Requests: New Request"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "agin-requests-sidebar",
          "title": "Agin Requests",
          "icon": "assets/sidebar.svg"
        }
      ]
    },
    "views": {
      "agin-requests-sidebar": [
        {
          "type": "webview",
          "id": "agin-requests-sidebar",
          "name": "Agin Requests",
          "icon": "assets/sidebar.svg",
          "contextualTitle": "Agin Requests"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@types/qs": "^6.9.16",
    "@types/vscode": "^1.95.0",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@bany/curl-to-json": "^1.2.8",
    "@vscode/codicons": "^0.0.36",
    "axios": "^1.7.7",
    "form-data": "^4.0.1",
    "qs": "^6.13.0"
  }
}
